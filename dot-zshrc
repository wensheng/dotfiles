export ZSH_DISABLE_COMPFIX=true
if [[ $OSTYPE == linux* ]]; then
  export ZSH="/home/wensheng/.oh-my-zsh"
else
  export ZSH="/Users/wensheng/.oh-my-zsh"
fi
ZSH_THEME="wensheng"
CASE_SENSITIVE="true"
DISABLE_AUTO_UPDATE="true"
DISABLE_AUTO_TITLE="true"
plugins=(
  gitfast
  python
  tmux
  z
  zsh-autosuggestions
)
source $ZSH/oh-my-zsh.sh
#autoload -Uz compinit && compinit
#source ~/.zsh/hashcmd-complete.zsh
## after loading the plugin
#ZSH_AUTOSUGGEST_STRATEGY=(history completion)
export GOPATH=$HOME/go
export PATH=$HOME/.local/bin:$PATH
unsetopt no_match
bindkey -v
bindkey '^R' history-incremental-pattern-search-backward
bindkey "^P" vi-up-line-or-history
bindkey "^N" vi-down-line-or-history
setopt nosharehistory
source ~/.aliases_common
if [[ $OSTYPE == linux* ]]; then
        source ~/.sh_linux
else
        source ~/.sh_mac
fi
export MAVEN_HOME=/opt/apache-maven-3.8.5
export CUDA_HOME=/usr/local/cuda
export PATH=$CUDA_HOME/bin:$HOME/bin:$PATH:/usr/local/go/bin:$HOME/go/bin:$MAVEN_HOME/bin:$PATH

# fnm
FNM_PATH="/home/wensheng/.local/share/fnm"
if [ -d "$FNM_PATH" ]; then
  export PATH="/home/wensheng/.local/share/fnm:$PATH"
  eval "`fnm env`"
fi
