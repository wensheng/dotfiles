if [[ $OSTYPE == linux* ]]; then
  export ZSH="/home/wensheng/.oh-my-zsh"
else
  export ZSH="/Users/wwang1/.oh-my-zsh"
fi
ZSH_THEME="wensheng"
CASE_SENSITIVE="true"
DISABLE_AUTO_UPDATE="true"
DISABLE_AUTO_TITLE="true"
plugins=(
  gitfast
  python
  tmux
  z
  zsh-autosuggestions
)
export GOPATH=$HOME/go
#export PATH=$HOME/.local/bin:$PATH
unsetopt no_match
bindkey -v
bindkey '^R' history-incremental-pattern-search-backward
bindkey "^P" vi-up-line-or-history
bindkey "^N" vi-down-line-or-history
setopt nosharehistory
source ~/.aliases_common
if [[ $OSTYPE == linux* ]]; then
        source ~/.sh_linux
else
        source ~/.sh_mac
fi
source $ZSH/oh-my-zsh.sh
export MAVEN_HOME=/opt/apache-maven-3.8.5
export PATH=$HOME/bin:/opt/nvim-linux-x86_64/bin:/usr/local/go/bin:$HOME/go/bin:$MAVEN_HOME/bin:$PATH

. "$HOME/.cargo/env"
source $HOME/.cargo/env

# fnm
FNM_PATH="/home/wensheng/.local/share/fnm"
if [ -d "$FNM_PATH" ]; then
  export PATH="/home/wensheng/.local/share/fnm:$PATH"
  eval "`fnm env`"
fi
