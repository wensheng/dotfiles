# libpq is keg-only and not symlinked into /usr/local because it has conflict with postgres
# mysql-client is keg-only and not symlinked into /usr/local because it has conflict with mysql
export PATH="/usr/local/opt/libpq/bin:/usr/local/opt/neovim/bin:/usr/local/opt/mysql-client/bin:$PATH"
# export LDFLAGS="-L/usr/local/opt/libpq/lib"
# export CPPFLAGS="-I/usr/local/opt/libpq/include"
# export LDFLAGS="-L/usr/local/opt/mysql-client/lib"
# export CPPFLAGS="-I/usr/local/opt/mysql-client/include"
# export PKG_CONFIG_PATH="/usr/local/opt/libpq/lib/pkgconfig"
# export PKG_CONFIG_PATH="/usr/local/opt/mysql-client/lib/pkgconfig"
test -e "${HOME}/.iterm2_shell_integration.zsh" && source "${HOME}/.iterm2_shell_integration.zsh"
alias dir='ls -lrtG'
alias df='df -H'
